{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "Get My TG",
  "colors": {
    "primary": "#3B6EC8",
    "light": "#5B8EE8",
    "dark": "#2B5EB8"
  },
  "icons": {
    "library": "lucide"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Документация",
        "groups": [
          {
            "group": "Начало работы",
            "pages": [
              "index",
              "getting-started/overview",
              "getting-started/first-steps"
            ]
          },
          {
            "group": "Аккаунты",
            "pages": [
              "accounts/structure",
              "accounts/sessions",
              "accounts/env",
              "api/overview"
            ]
          },
          {
            "group": "Сеть",
            "pages": ["network/overview", "network/proxy"]
          },
          {
            "group": "Остальное",
            "pages": ["security/overview", "issues/troubleshooting", "glossary"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": {
          "source": "https://api.getmytg.com/docs/openapi.json",
          "directory": "api-reference"
        }
      },
      {
        "tab": "SDK",
        "openapi": {
          "source": "https://app.stainless.com/api/spec/documented/gmt/openapi.documented.yml",
          "directory": "sdk-reference"
        }
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Main Bot",
          "href": "https://t.me/GetMyTGrobot",
          "icon": "bot"
        },
        {
          "anchor": "Contact Support",
          "href": "https://t.me/GetMyTGSUPbot",
          "icon": "message-circle"
        },
        {
          "anchor": "OpenAPI (JSON)",
          "href": "https://api.getmytg.com/docs/openapi.json",
          "icon": "download"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/cameo6",
      "telegram": "https://t.me/GetMyTGrobot"
    }
  }
}
